                        %span
                          JS
                  .col-xs-1
                    %span.line-35
                      30%
                .row
                  .col-xs-11
                    .progress
                      .progress-bar.blue{role:"progressbar", "aria-valuenow" => "60", "aria-valuemin" => "0", "aria-valuemax" => "100", style: "width: 95%"}
                        %span
                          jQuery
                  .col-xs-1
                    %span.line-35
                      60%
              .tab-pane#profile{role: "tabpanel"}
                .row
                  .col-xs-12
                    %span.circle.blue
                      = image_tag "devops_icon.png"
                    %span.font-20
                      Devops
                %p
                  %br
                    %h4
                      %em
                        %strong
                          "
